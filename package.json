  "private": true,
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:functions": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.functions.config.mjs",
    "test:functions:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.functions.config.mjs --watch",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules FIREBASE_EMULATOR=true jest --config jest.functions.config.mjs __tests__/integration",
