    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-web-notification": "^0.7.0",
    "reactstrap": "^9.0.0",
    "sass": "^1.77.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@swc/plugin-styled-jsx": "^13.5.0",
